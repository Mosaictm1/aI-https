{
    "name": "ai-http-backend",
    "version": "1.0.0",
    "description": "AI-HTTP Backend API - مساعد HTTP الذكي لـ n8n",
    "main": "dist/main.js",
    "scripts": {
        "dev": "tsx watch src/main.ts",
        "build": "prisma generate && prisma migrate deploy && tsc",
        "start": "node dist/main.js",
        "start:prod": "node dist/main.js",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:push": "prisma db push",
        "prisma:studio": "prisma studio",
        "prisma:seed": "tsx prisma/seed.ts"
    },
    "keywords": [
        "n8n",
        "http",
        "api",
        "debugging",
        "ai"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.7.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.5",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "bull": "^4.12.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "prisma": "^5.7.0",
        "socket.io": "^4.7.2",
        "typescript": "^5.3.3",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "tsx": "^4.7.0",
        "vitest": "^1.1.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}